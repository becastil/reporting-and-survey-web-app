# Database Configuration
DATABASE_URL="postgresql://user:password@localhost:5432/survey_platform_dev"

# Optional: Redis Configuration (for caching)
REDIS_URL="redis://localhost:6379"

# Authentication (Clerk) - Get keys from https://clerk.dev
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="pk_test_your_key_here"
CLERK_SECRET_KEY="sk_test_your_secret_here"

# Application
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"

# Demo Mode - Set to true to bypass authentication
DEMO_MODE="false"
SKIP_AUTH="false"
SKIP_ENV_VALIDATION="false"

# Feature Flags
ENABLE_DEMO_MODE="false"
ENABLE_PERFORMANCE_BADGES="true"
ENABLE_PEER_COMPARISON="true"